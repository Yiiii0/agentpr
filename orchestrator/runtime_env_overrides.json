{
  "AGENTPR_RUNTIME_DIR": "{runtime_dir}",
  "AGENTPR_WORKSPACE_DIR": "{repo_dir}",
  "XDG_CACHE_HOME": "{cache_dir}",
  "XDG_DATA_HOME": "{data_dir}",
  "TMPDIR": "{tmp_dir}",
  "PIP_CACHE_DIR": "{cache_dir}/pip",
  "PIP_DISABLE_PIP_VERSION_CHECK": "1",
