# AgentPR runtime
AGENTPR_BASE_DIR=/Users/yi/Documents/Career/TensorBlcok/agentpr/workspaces

# Telegram bot
AGENTPR_TELEGRAM_BOT_TOKEN=

# Telegram NL routing mode: rules | hybrid | llm
AGENTPR_TELEGRAM_NL_MODE=hybrid
AGENTPR_TELEGRAM_NL_MODEL=gpt-4o-mini
AGENTPR_TELEGRAM_NL_API_BASE=
AGENTPR_TELEGRAM_NL_API_KEY_ENV=AGENTPR_MANAGER_API_KEY
AGENTPR_TELEGRAM_NL_TIMEOUT_SEC=20
AGENTPR_TELEGRAM_NOTIFY_ENABLED=1
AGENTPR_TELEGRAM_NOTIFY_SCAN_SEC=30
AGENTPR_TELEGRAM_NOTIFY_SCAN_LIMIT=200

# Manager LLM (manager loop + NL router fallback defaults)
AGENTPR_MANAGER_API_KEY=
AGENTPR_MANAGER_MODEL=gpt-4o-mini
AGENTPR_MANAGER_API_BASE=https://api.openai.com/v1

# Worker base prompt (used by manager-tick/run-manager-loop when --prompt-file is omitted)
AGENTPR_WORKER_PROMPT_FILE=forge_integration/claude_code_prompt.md
AGENTPR_DEFAULT_PROMPT_VERSION=v1
AGENTPR_CREATE_AUTOKICK=1

# GitHub webhook
AGENTPR_GITHUB_WEBHOOK_SECRET=

# Forge provider for codex worker (optional)
# If both BASE_URL and API_KEY are set, codex uses Forge instead of default provider.
# If unset, codex uses ~/.codex/config.toml defaults (e.g. OpenAI).
AGENTPR_FORGE_BASE_URL=
AGENTPR_FORGE_API_KEY=
AGENTPR_FORGE_MODEL=

# Optional codex binary override
AGENTPR_CODEX_BIN=
